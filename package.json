{
  "name": "v-lazy-show",
  "type": "module",
  "version": "0.2.2",
  "packageManager": "pnpm@8.1.0",
  "description": "Compile-time directive to lazy initialize v-show for Vue",
  "author": "Anthony Fu <anthonyfu117@hotmail.com>",
  "license": "MIT",
  "funding": "https://github.com/sponsors/antfu",
  "homepage": "https://github.com/antfu/v-lazy-show#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/antfu/v-lazy-show.git"
  },
  "bugs": "https://github.com/antfu/v-lazy-show/issues",
  "keywords": [],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    },
    "./nuxt": {
      "types": "./dist/nuxt.d.ts",
      "require": "./dist/nuxt.cjs",
      "import": "./dist/nuxt.mjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist",
    "*.d.ts"
  ],
  "scripts": {
    "build": "unbuild",
    "stub": "unbuild --stub",
    "dev": "nr -C playgrounds/vite dev",
    "lint": "eslint .",
    "prepublishOnly": "nr build",
    "release": "bumpp && npm publish",
    "start": "esno src/index.ts",
    "test": "vitest",
    "typecheck": "vue-tsc --noEmit"
  },
  "dependencies": {
    "@vue/compiler-core": "^3.2.47"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.38.2",
    "@antfu/ni": "^0.21.2",
    "@antfu/utils": "^0.7.2",
    "@types/node": "^18.15.11",
    "bumpp": "^9.1.0",
    "eslint": "^8.37.0",
    "esno": "^0.16.3",
    "lint-staged": "^13.2.0",
    "pnpm": "^8.1.0",
    "rimraf": "^4.4.1",
    "simple-git-hooks": "^2.8.1",
    "typescript": "^5.0.3",
    "unbuild": "^1.2.0",
    "vite": "^4.2.1",
    "vite-plugin-inspect": "^0.7.18",
    "vitest": "^0.29.8",
    "vue": "^3.2.47",
    "vue-tsc": "^1.2.0"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
